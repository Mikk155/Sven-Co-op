@PointClass base(Targetname, Master) color(0 255 255) = game_time : "Map Global Time"
[
    // using a value of 60 mean that one minute (in-game) is equal to one minute (in real life)
    // using a value of 1  mean that one minute (in-game) is equal to one second (in real life)
    health(string) : "One minute is (IRL-seconds)" : "60"
    
    // Internal values that will be updated by the entity current time and can be set though changevalue.
    current_second(integer) : "Current Second" : 0
    trigger_second(target_destination) : "Trigger when a second increase (USE_TYPE)"

