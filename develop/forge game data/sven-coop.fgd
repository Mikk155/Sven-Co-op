// -Mikk feature for triggering its target once per player/monster.
// NOTE: is not compatible with trigger_multiple's new spawnflag.
// since both works by changing its target and using a trigger_script as intermediary.
@BaseClass = TriggerIndividual
[
    $i_trigger_individual(choices) : "[AS] Trigger only once per activator" : 0 =
    [
        0: "Disabled"
        1: "Enabled"
    ]
]

@BaseClass = MonsterAdditionsAS
[
    // -Mikk feature for passing a monster's Attacker/damage inflictor as a !activator.
    $i_damage_inflictor(choices) : "[AS] TriggerTarget's !activator" : 0 =
    [
        0: "This monster"
        1: "Damage Inflictor/Attacker"
    ]
    // -Mikk prevent a monster from generating blood puddles
    $f_bloodpuddle(choices) : "[AS] Generate blood puddle when die" : 0 =
    [
        0: "Yes"
        1: "No"
    ]
    // -Mikk blood puddle skin value for this monster. if empty = use bloodcolor from monster to set red or yellow.
    $i_bloodpuddle(integer) : "[AS] blood puddle skin"
]

// -Mikk feature for preventing a model/item being replaced with entity config_classic_mode
@BaseClass = ClassicModeIgnore
