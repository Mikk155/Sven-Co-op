    trigger_minute(target_destination) : "Trigger when a minute increase (USE_TYPE)"

    current_hour(integer) : "Current Hour" : 0
    trigger_hour(target_destination) : "Trigger when a hour increase"

    current_day(integer) : "Current Day" : 0
    trigger_day(target_destination) : "Trigger when a day increase (USE_TYPE)"

    // targetname of a light_spot to change its pattern depending the time. if "!world" it'll be a global change.
    light_pattern(target_destination) : "light to change pattern" : "!world"
    
