void PluginInit()
{
    g_Module.ScriptInfo.SetAuthor
	(
		"Mikk https://github.com/Mikk155 & Gaftherman https://github.com/Gaftherman"
	);
    g_Module.ScriptInfo.SetContactInfo
    (
        "Discord: https://discord.gg/VsNnE3A7j8"
    );

    g_Hooks.RegisterHook( Hooks::Player::ClientSay, @ClientSay );
}

// Numero de los packs respectivamente -Mikk
// 0 = Classic Weapons.- 
// 1 = Afraid Of Monsters Weapons.-
// 2 = Cry Of Fear Weapons.-
// 3 = Counter-Strike 1.6 Weapons.-
// 4 = Insurgency 2 WW2 topic Weapons pack.-
// 5 = Insurgency 2 Modern topic Weapons pack.-
// 6 = Insurgency 2 Alternative Weapons pack.-
int WeaponChoice = 0;

HookReturnCode ClientSay( SayParameters@ pParams )
{
    CBasePlayer@ pPlayer = pParams.GetPlayer();
    const CCommand@ pArguments = pParams.GetArguments();

    if ( pArguments.ArgC() >= 1 )
    {
		// El vote me lo invento luego. escribir '/pack (numero)' para cambiarlo -Mikk
        if( pArguments[0] == "/pack" )
        {
			if( pArguments[1] >= 0 && pArguments[1] <= 6 )
			{
				WeaponChoice = pArguments[1];
                g_PlayerFuncs.ClientPrint( pPlayer, HUD_PRINTTALK, "[AS] Se cambio el pack de armas a '"+WeaponChoice+"' al reiniciar se abrira otro ent\n" );
			}
        }
    }
    return HOOK_CONTINUE;
}

